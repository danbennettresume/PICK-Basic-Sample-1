DANB.CORRECT.JAN.2016.FORMAT.ISSUE
$UDI I_COMMON
* DANB    NO POINT, JUST TRYING TO SAVE ONTO GITHUB DIFFENTLY
*
PRINT "ENTER THE TEST " :
INPUT WORD.IN
WORD.LEN = LEN(WORD.IN)
FOR I = WORD.LEN TO 1 STEP -1
  WORD.FLIPPED := WORD.IN[I]
NEXT I
IF WORD.IN = WORD.FLIPPED THEN
  PRINT "PALINDROME"
END ELSE
  PRINT "NOT PALINDROME"
END
STOP
